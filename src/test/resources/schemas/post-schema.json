{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["userId", "id", "title", "body"],
    "properties": {
        "userId": {
            "type": "integer",
            "minimum": 1
        },
        "id": {
            "type": "integer",
            "minimum": 1
        },
        "title": {
            "type": "string",
            "minLength": 1
        },
        "body": {
            "type": "string",
            "minLength": 1
        }
    }
} 